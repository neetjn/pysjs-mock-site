{
  "name": "pysjs-mock-site",
  "version": "1.0.0",
  "description": "pyseleniumjs testing mock site",
  "main": "index.js",
  "scripts": {
    "setup": "npm install && npm run build",
    "build": "node_modules/.bin/cross-env NODE_ENV=production NODE_ENV=production node_modules/.bin/webpack --config build/webpack.conf.js",
    "build:dev": "node_modules/.bin/webpack --config build/webpack.conf.js",
    "app": "node_modules/.bin/webpack-dev-server --config build/webpack.conf.js --port 3000",
    "app:detached": "nohup npm run app &",
    "lint": "node_modules/.bin/eslint src/**.js",
    "test": "npm run lint"
  },
  "keywords": [
    "selenium",
    "js",
    "python"
  ],
  "author": "John Nolette",
  "license": "MIT",
  "devDependencies": {
    "angular": "^1.5.2",
    "angularjs-template-loader": "^0.1.1",
    "angular-ui-router": "^1.0.3",
    "animate.css": "^3.5.2",
    "bulma": "^0.4.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.22.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^4.1.1",
    "eslint-loader": "^1.6.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "font-awesome": "^4.7.0",
    "node-sass": "^4.0.0",
    "npm": "^5.0.4",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.0",
    "url-loader": "^0.5.9",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-dev-server": "^2.5.0"
  }
}
